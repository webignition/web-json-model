{
    "name": "webignition/web-json-model",
    "description": "PHP model of a web-based JSON resource, based on webignition/web-resource-model",
    "keywords": ["model", "json"],
    "homepage": "https://github.com/webignition/web-json-model",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "jon@webignition.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\WebResource\\JsonDocument\\": "src/",
            "webignition\\Tests\\WebResource\\JsonDocument\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always",
        "cs": "./vendor/bin/phpcs src tests --colors --standard=PSR2",
        "ci": [
            "@composer cs",
            "@composer test"
        ]
    },
    "require": {
        "php": ">=5.5.0",
        "webignition/web-resource-model":">=1.2.1,<2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "3.*",
        "mockery/mockery": "^0.9"
    },
    "minimum-stability":"dev",
    "prefer-stable":true
}